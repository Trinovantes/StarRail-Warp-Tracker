{
    "type": "module",
    "version": "0.3.13",
    "appId": "ca.stephenli.starrail-warp-tracker",
    "name": "starrail-warp-tracker",
    "productName": "StarRail Warp Tracker",
    "description": "Windows application to track and backup your Honkai StarRail warp history",
    "homepage": "https://github.com/Trinovantes/StarRail-Warp-Tracker",
    "author": {
        "name": "Stephen Li",
        "email": "hello@stephenli.ca",
        "url": "https://www.stephenli.ca"
    },
    "private": true,
    "main": "./dist/main.cjs",
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "dev": "  cross-env NODE_ENV=development node ./build/runDev.ts",
        "build": "cross-env NODE_ENV=production  node ./build/runBuild.ts",
        "download": "  cross-env ELECTRON_RUN_AS_NODE=1 electron --import=./build/preload.ts ./src/scripts/downloadAssets.ts",
        "fetchWarps": "cross-env ELECTRON_RUN_AS_NODE=1 electron --import=./build/preload.ts ./src/scripts/fetchWarps.ts",
        "unit": "      cross-env ELECTRON_RUN_AS_NODE=1 electron node_modules/vitest/vitest.mjs",
        "lint": "vue-tsc && eslint",
        "test": "pnpm unit"
    },
